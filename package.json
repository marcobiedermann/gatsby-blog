  ],
  "license": "MIT",
  "scripts": {
    "build": "gatsby build",
    "clean": "gatsby clean",
    "develop": "gatsby develop",
    "format": "prettier --ignore-path .gitignore --write .",
    "lint:css": "stylelint --ignore-path .gitignore '**/*.css'",
    "lint:js": "eslint --ignore-path .gitignore '**/*.{js,jsx,ts,tsx}'",
    "lint": "npm run lint:css && npm run lint:js",
    "serve": "gatsby serve",
    "start": "npm run develop",
    "test": "jest --runInBand",
    "test:ci": "npm run test -- --color --ci"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/marcobiedermann/gatsby-blog"
  },
  "bugs": {
    "url": "https://github.com/marcobiedermann/gatsby-blog/issues"
  }
